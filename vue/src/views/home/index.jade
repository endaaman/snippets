section.home
  h2 count
  p
    | count: {{count}}
  ul
    li
      router-link(to="/") Home
    li
      router-link(to="/hoge") Hoge
  p
    button(@click="increment(10)")
      | Increment
  hr
  h2 todo
  ul
    li(v-for="todo in todos.items")
      label
        input(type="checkbox" v-model="selectedTodoIds" v-bind:value="todo.id")
        | id: {{todo.id}}
        | text: {{todo.text}}
        | done: {{todo.done ? 'DONE' : 'NOT DONE'}}

  el-table(ref='multipleTable', :data='todos.items', border='', style='width: 100%', @selection-change='onSelected')
      el-table-column(type='selection', width='55')
      el-table-column(property='id', label='ID', width='120')
      el-table-column(property='text', label='Text', show-overflow-tooltip='')
      el-table-column(label='Done', width='120')
        template(scope='scope') {{ scope.row.done ? 'âœ“' : '' }}

  p
    button(@click="done") mark done
    span &nbsp;
    button(@click="undone") mark undone

